create table if not exists Genres(
	id serial primary key,
	name varchar(30)
);

create table if not exists Authorslist(
	id serial primary key,
	name varchar(30) not null unique
);

create table if not exists Albums(
	id serial primary key,
	name varchar(30) not null unique,
	realease date
);

create table if not exists Tracks(
    	id serial primary key,
	id serial primary key not null references Albums(id),
	name varchar(30) not null unique,
	time integer
);

create table if not exists Authorslist_albums(
    	primary key(authorList_id, album_id),
	author_id integer references Authorslist(id),
	album_id integer references Albums(id)
);

create table if not exists Authorslist_Genres(
    	primary key(authorlist_id, genres_id),
	genre_id integer references Genres(id),
	author_id integer references Authorslist(id)
);

create table if not exists Collections(
    	primary key(authorlist_id, tracks_id),
	tracks_id integer references Tracks(id),
	author_id integer references Authorslist(id),
	name varchar(50) not null,
	realease date
);
